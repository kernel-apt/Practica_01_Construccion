<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="pane" styleClass="pane-class" stylesheets="@../css/home.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HomeController">
   <children>
      <ImageView id="image-view" fx:id="imgViewProfile" fitHeight="250.0" fitWidth="254.0" layoutX="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icon/Pasted%20image.png" />
         </image></ImageView>
      <TableView fx:id="tableViewItems" layoutX="252.0" styleClass="table-class">
        <columns>
          <TableColumn fx:id="colItemName" prefWidth="75.0" styleClass="column-class" text="Item" />
          <TableColumn fx:id="colPrice" prefWidth="75.0" styleClass="column-class" text="Precio" />
            <TableColumn fx:id="colQuantity" prefWidth="75.0" styleClass="column-class" text="Cantidad" />
        </columns>
      </TableView>
      <Button fx:id="btnGoToProfile" layoutX="3.0" layoutY="257.0" mnemonicParsing="false" onAction="#GoToProfile" styleClass="button-class" text="Perfil" />
   </children>
</Pane>
